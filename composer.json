{
    "name": "AsyrafHussin/laravel-shopping-cart",
    "description": "Shopping cart for Laravel Application.",
    "keywords": [
        "shopping cart",
        "shopping",
        "laravel"
    ],
    "authors": [
        {
            "name": "AsyrafHussin",
            "email": "anzhengchao@gmail.com"
        }
